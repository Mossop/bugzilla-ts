{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "es2019",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noEmit": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "incremental": true,

    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true
  }
}
